state 
	start,
	window18 { plan_clock <= 10 and window_clock <= 10 },
	window19 { plan_clock <= 40 and window_clock <= 30 },
	window20 { plan_clock <= 50 and window_clock <= 10 },
	window21 { plan_clock <= 100 },
