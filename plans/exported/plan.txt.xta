			 R32_clock  := H ,
			 R9_clock  := H ;  },
	pointingmode16 -> pointingmode17  { guard plan_clock >= 35 and pointingmode_clock >= 1
			and  R32_clock  >= H + 0 and  R32_clock  <= H + 100
