clock plan_clock, R1_clock, R2_clock, R3_clock, R4_clock, R5_clock, R6_clock, R7_clock, R8_clock, R9_clock, R10_clock, R11_clock, R12_clock, R13_clock, R14_clock, R15_clock, R16_clock, R17_clock, R18_clock, R19_clock, R20_clock, R21_clock, R22_clock, R23_clock, R24_clock, R25_clock, R26_clock, R27_clock, R28_clock, R29_clock, R30_clock, R31_clock, R32_clock;
