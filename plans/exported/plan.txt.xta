process pointingmode ()
{
clock pointingmode_clock;
state 
	start,
	pointingmode0 { plan_clock <= 5 and pointingmode_clock <= 5 },
	pointingmode1 { plan_clock <= 8 and pointingmode_clock <= 3 },
	pointingmode2 { plan_clock <= 11 and pointingmode_clock <= 3 },
	pointingmode3 { plan_clock <= 14 and pointingmode_clock <= 3 },
	pointingmode4 { plan_clock <= 17 and pointingmode_clock <= 3 },
	pointingmode5 { plan_clock <= 20 and pointingmode_clock <= 3 },
	pointingmode6 { plan_clock <= 23 and pointingmode_clock <= 3 },
	pointingmode7 { plan_clock <= 26 and pointingmode_clock <= 3 },
	pointingmode8 { plan_clock <= 29 and pointingmode_clock <= 3 },
	pointingmode9 { plan_clock <= 32 and pointingmode_clock <= 3 },
	pointingmode10 { plan_clock <= 33 and pointingmode_clock <= 5 },
	pointingmode11 { plan_clock <= 34 and pointingmode_clock <= 5 },
	pointingmode12 { plan_clock <= 35 and pointingmode_clock <= 5 },
	pointingmode13 { plan_clock <= 36 and pointingmode_clock <= 5 },
