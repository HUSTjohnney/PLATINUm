		    assign pointingmode_clock := 0; },
	pointingmode4 -> pointingmode5  { guard plan_clock >= 13 and pointingmode_clock >= 3;
			assign pointingmode_clock := 0,
			 R25_clock  := H ,
			 R19_clock  := H ,
			 R16_clock  := H ,
			 R7_clock  := H ;  },
	pointingmode5 -> pointingmode6  { guard plan_clock >= 16 and pointingmode_clock >= 3
			and  R19_clock  >= H + 0 and  R19_clock  <= H + 100
