		    assign pointingmode_clock := 0; },
	pointingmode6 -> pointingmode7  { guard plan_clock >= 19 and pointingmode_clock >= 3;
			assign pointingmode_clock := 0,
			 R28_clock  := H ,
			 R27_clock  := H ,
			 R21_clock  := H ,
			 R20_clock  := H ,
			 R4_clock  := H ;  },
	pointingmode7 -> pointingmode8  { guard plan_clock >= 22 and pointingmode_clock >= 3
			and  R27_clock  >= H + 0 and  R27_clock  <= H + 100
			and  R26_clock  >= H + 0 and  R26_clock  <= H + 100
			and  R24_clock  >= H + 0 and  R24_clock  <= H + 100;
		    assign pointingmode_clock := 0; },
	pointingmode8 -> pointingmode9  { guard plan_clock >= 25 and pointingmode_clock >= 3;
			assign pointingmode_clock := 0,
			 R29_clock  := H ,
			 R1_clock  := H ;  },
	pointingmode9 -> pointingmode10  { guard plan_clock >= 28 and pointingmode_clock >= 3;
			assign pointingmode_clock := 0; },
	pointingmode10 -> pointingmode11  { guard plan_clock >= 29 and pointingmode_clock >= 1
			and  R20_clock  >= H + 0 and  R20_clock  <= H + 100
			and  R16_clock  >= H + 0 and  R16_clock  <= H + 100
			and  R2_clock  >= H + 0 and  R2_clock  <= H + 100
			and  R1_clock  >= H + 0 and  R1_clock  <= H + 100;
		    assign pointingmode_clock := 0; },
	pointingmode11 -u-> pointingmode12  { guard plan_clock >= 30 and pointingmode_clock >= 1;
			assign pointingmode_clock := 0,
			 R30_clock  := H ,
			 R22_clock  := H ,
			 R17_clock  := H ,
			 R3_clock  := H ;  },
	pointingmode12 -> pointingmode13  { guard plan_clock >= 31 and pointingmode_clock >= 1
			and  R17_clock  >= H + 0 and  R17_clock  <= H + 100
			and  R5_clock  >= H + 0 and  R5_clock  <= H + 100
			and  R4_clock  >= H + 0 and  R4_clock  <= H + 100;
		    assign pointingmode_clock := 0; },
	pointingmode13 -u-> pointingmode14  { guard plan_clock >= 32 and pointingmode_clock >= 1;
